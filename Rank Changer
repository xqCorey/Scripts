--server sided rank changer. spts

while wait do
wait (.2) -- how fast to switch between ranks
local Remote = game.ReplicatedStorage['RemoteEvent']

local Arguments = {
       [1] = {
           [1] = "ChangeRankEmblem",
           [2] = math.random (1,9) --1 = min rank 12 = max rank
       }
}

Remote:FireServer(unpack(Arguments))
end
